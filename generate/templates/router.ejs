import express from 'express';
import { read<%= appName.pascal %>s, read<%= appName.pascal %>, create<%= appName.pascal %>, update<%= appName.pascal %>, delete<%= appName.pascal %> } from './<%= appName.original %>.controller.js';

const router = express.Router();

router.get('/', read<%= appName.pascal %>s);
router.get('/:id', read<%= appName.pascal %>);
router.post('/', create<%= appName.pascal %>);
router.put('/:id', update<%= appName.pascal %>);
router.delete('/:id', delete<%= appName.pascal %>);

export default router;