// templates/router.ejs
import express from 'express';
import <%= camel %>controller from './<%= original %>.controller.js';

const router = express.Router();

router.get('/<%= pluralLower %>', <%= camel %>controller.list);
router.get('/<%= pluralLower %>/:id', <%= camel %>controller.get);
router.post('/<%= pluralLower %>', <%= camel %>controller.create);
router.put('/<%= pluralLower %>/:id', <%= camel %>controller.update);
router.delete('/<%= pluralLower %>/:id', <%= camel %>controller.remove);

export default router;